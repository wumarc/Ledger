<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Ledger.ledger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ledger">
        <activity android:name="com.Ledger.ledger.Activity.DeleteAccountActivity"></activity>
        <activity android:name="com.Ledger.ledger.Activity.Signup" />
        <activity android:name="com.Ledger.ledger.Activity.ItemDetailsActivity" />
        <activity android:name="com.Ledger.ledger.Activity.MenuActivity" />
        <activity android:name="com.Ledger.ledger.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>